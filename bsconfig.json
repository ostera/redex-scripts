{
  "name": "redex-scripts",
  "package-specs": [{
    "module": "commonjs",
    "in-source": true
  }],
  "sources": [{
    "dir": "src",
    "subdirs": true
  }],
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "refmt": 3,
  "bs-dependencies": ["@glennsl/bs-json", "refetch", "@glennsl/rebase"],
  "js-post-build": {
    "cmd": "/usr/bin/sed -i '1 i\\#!/usr/bin/env node'"
  }
}